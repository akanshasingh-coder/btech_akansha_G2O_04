#include <stdio.h>
int main()
{
    int p,c,m,e,h;
    float P;
    printf("enter the marks in physics \n");
    scanf("%d",&p);
    printf("enter the marks in chemistry \n");
    scanf("%d",&c);
    printf("enter the marks in maths \n");
    scanf("%d",&m);
    printf("enter the marks in english \n");
    scanf("%d",&e);
    printf("enter the marks in hindi \n");
    scanf("%d",&h);
    P=(p+c+m+e+h)/5;
    if (P>=90)
    {
        printf("Grade:A");
    }
    else if (P>=80&&P<90)
    {
        printf("Grade:B");
    }
    else if (p>=60&&p<80)
    {
        printf("Grade:C");
    }
    else if (p<60)
    {
        printf("Grade:D");
    }
    return 0;
}
