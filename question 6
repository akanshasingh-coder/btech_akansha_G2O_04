#include <stdio.h>
int main()
{
    float a,b,c;
    printf("enter the three numbers");
    scanf("%f %f %f",&a,&b,&c);
    if (a>b)
    {
        if (a>c){
            printf(" the greater number is %f",a);
        }
        else {
            printf("the greater number is %f",c);
        }
    }
    else if(b>c){
        printf("the greater number is %f",b);
    }
    return 0;
}
